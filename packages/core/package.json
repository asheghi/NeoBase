{
  "name": "@neobase/core",
  "title": "NeoBase",
  "version": "0.0.1",
  "description": "NeoBase Server",
  "main": "dist/index.js",
  "bin": {
    "neobase": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/cli/index.ts start --debug --cookie-secret nosecret",
    "cli": "node dist/index.js",
    "build": "npm run clear:dist && npm run esbuild",
    "lint": "eslint src/**/*.{ts,js,cjs}",
    "lint:fix": "eslint src/**/*.{ts,js,cjs} --fix",
    "debug": "node --inspect -r ts-node/register src/cli/index start --debug --cookie-secret nosecret",
    "esbuild": "node scripts/EsBuild.cjs",
    "test-prod": "npm run build && npm run start",
    "clear:dist": "rimraf dist"
  },
  "dependencies": {
    "@neobase/admin-ui": "workspace:*",
    "@neobase/client": "workspace:*",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "commander": "^10.0.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug-level": "^2.1.2",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-slow-down": "^1.4.0",
    "figlet": "^1.5.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.3",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "rimraf": "^2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/connect-flash": "^0.0.37",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.6",
    "@types/express-slow-down": "^1.3.2",
    "@types/figlet": "^1.5.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.12",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.16.0",
    "fs-extra": "^11.1.0",
    "ts-node-dev": "^2.0.0"
  }
}