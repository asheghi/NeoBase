{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "lib": ["esnext", "dom", "DOM.Iterable"],
    "strict": false,
    "sourceMap": false,
    "resolveJsonModule": true,
